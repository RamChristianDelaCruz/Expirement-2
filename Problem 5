#include <iostream>
#include <conio.h>

using namespace std;

int main()
{
cout<<"Enter a key to start the program "<<endl;
_getch();
int number;

for (number = 1;number <=30; number++)
{
if (number == 11) {continue;}
if (number == 13) {continue;}
if (number == 15) {continue;}
if (number == 17) {continue;}
if (number == 19) {continue;}
if (number == 21) {continue;}
if (number == 23) {continue;}
if (number == 25) {continue;}
if (number == 27) {continue;}
if (number == 29) {continue;}
if (number == 30) {cout<<number; break;}
_getch();
cout<< number<<",";

}


_getch();
return 0;
}
