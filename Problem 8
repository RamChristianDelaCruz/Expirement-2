#include <iostream>

using namespace std;

int main()
{
    int a;
    
    cout<< "Enter an integer: ";cin>>a;
    
    cout<<"1*"<<a<<" = "<<a*1<<endl; 
    cout<<"2*"<<a<<" = "<<a*2<<endl; 
    cout<<"3*"<<a<<" = "<<a*3<<endl; 
    cout<<"4*"<<a<<" = "<<a*4<<endl; 
    cout<<"5*"<<a<<" = "<<a*5<<endl; 
    cout<<"6*"<<a<<" = "<<a*6<<endl; 
    cout<<"7*"<<a<<" = "<<a*7<<endl; 
    cout<<"8*"<<a<<" = "<<a*8<<endl; 
    cout<<"9*"<<a<<" = "<<a*9<<endl; 
    cout<<"10*"<<a<<" = "<<a*10<<endl; 

    return 0;
}
